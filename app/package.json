{
  "name": "react-asset-management",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npx vite --mode development-local",
    "build-production": "echo Building for production && npm run lint && npm run test && npx tsc && npx vite build",
    "build-development": "echo Building for development && npm run lint && npm run test && npx cross-env NODE_ENV=development npx tsc && npx cross-env NODE_ENV=development npx vite build --mode development",
    "test": "npx vitest run",
    "test-watch": "npx vitest",
    "force-build-production": "echo Force building for production && npx tsc && npx vite build",
    "force-build-development": "echo Force building for development && npx cross-env NODE_ENV=development npx tsc && npx cross-env NODE_ENV=development npx vite build --mode development",
    "lint": "npx eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 && npx prettier . --check --ignore-unknown",
    "preview": "npx vite preview",
    "format": "npx prettier . --write --ignore-unknown",
    "generate-rest-client-lisp": "npx @openapitools/openapi-generator-cli generate -i /local/openapi/lisp/spec.json -g typescript-rxjs -o /local/openapi/lisp/generated/",
    "storybook": "npx storybook dev -p 6006",
    "build-storybook": "npx storybook build"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.12",
    "@mui/icons-material": "^5.15.14",
    "@mui/material": "^5.15.14",
    "@mui/x-charts": "^7.0.0",
    "@reduxjs/toolkit": "^2.8.2",
    "@tk/tk-common-components": "^1.0.23",
    "@tk/tk-intl": "^1.0.5",
    "@tk/tk-layouts": "^1.0.9",
    "@tk/tk-navbar": "^1.0.9",
    "@tk/tk-search": "^1.0.9",
    "@tk/tk-themes": "^1.0.8",
    "@tk/tk-webvitals": "^1.0.3",
    "ag-grid-community": "^31.2.0",
    "ag-grid-enterprise": "^31.2.0",
    "ag-grid-react": "^31.2.0",
    "clsx": "^2.1.0",
    "highcharts": "^12.3.0",
    "highcharts-react-official": "^3.2.2",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.6.3",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.3",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.2.25",
    "@openapitools/openapi-generator-cli": "^2.13.1",
    "@storybook/addon-essentials": "^8.0.4",
    "@storybook/addon-interactions": "^8.0.4",
    "@storybook/addon-links": "^8.0.4",
    "@storybook/addon-onboarding": "^8.0.4",
    "@storybook/blocks": "^8.0.4",
    "@storybook/react": "^8.0.4",
    "@storybook/react-vite": "^8.0.4",
    "@storybook/test": "^8.0.4",
    "@types/react": "^18.2.71",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-formatjs": "^4.12.2",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-storybook": "^0.8.0",
    "lint-staged": "^15.2.2",
    "openapi-typescript": "^6.7.5",
    "postcss": "^8.4.38",
    "storybook": "^8.0.4",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.3",
    "vite": "^5.2.6",
    "vite-plugin-static-copy": "^1.0.2",
    "vitest": "^1.4.0"
  }
}
